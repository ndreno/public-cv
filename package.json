{
    "name": "public-cv-generator",
    "version": "1.0.0",
    "description": "Generates HTML resumes from JSON files.",
    "scripts": {
        "build:en": "resume export --format html --theme ./themes/jsonresume-theme-cora-master-customized resumes/en.json > build/index.html",
        "build:fr": "resume export --format html --theme ./themes/jsonresume-theme-cora-master-customized resumes/fr.json > build/fr.html"
    },
    "dependencies": {
        "resume-cli": "latest"
    }
}